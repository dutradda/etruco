	  action, STATE_SET "default" 1.0;
	  target, "button-bg-go";
	  after, "do-go";
	}
	program {
	  name, "do-go";
	  source, "button-go_unclicked";
	  action, SIGNAL_EMIT "enter_screen_waiting" "";
