      } /* Close Parts */
      programs {
	/* LEAVE BUTTON */
	program {
	  name, "button-leave_in";
	  signal, "mouse,in";
	  source, "button-bg-leave";
	  action, STATE_SET "default" 1.0;
	  target, "button-bg-leave";
	}
	program {
	  name, "button-leave_out";
	  signal, "mouse,out";
	  source, "button-bg-leave";
	  action, STATE_SET "default" 0.0;
	  target, "button-bg-leave";
	}
	program {
	  name, "button-leave_clicked";
	  signal, "mouse,down,1";
	  source, "button-bg-leave";
	  action, STATE_SET "default" 0.0;
	  target, "button-bg-leave";
	}
	program {
	  name, "button-leave_unclicked";
	  signal, "mouse,up,1";
	  source, "button-bg-leave";
	  action, STATE_SET "default" 1.0;
	  target, "button-bg-leave";
	  after, "do-leave";
	}
	program {
	  name, "do-leave";
	  source, "button-leave_unclicked";
	  action, SIGNAL_EMIT "leave_join" "";
	}

	/* ENTER BUTTON */
	program {
	  name, "button-go_in";
